@import 'jeet'

nav ul li
    +below(680px)
        padding 3px 3px


.left
    column(2/3)
    margin-top 10%
    -ms-transform: rotate(90deg); /* IE 9 */
    -webkit-transform: rotate(90deg);  Chrome, Safari, Opera
    transform: rotate(90deg);
    +mobile()
        stack()

.right
    column(1/3)
    margin-top 10%
    text-align center
    +mobile()
        stack()


